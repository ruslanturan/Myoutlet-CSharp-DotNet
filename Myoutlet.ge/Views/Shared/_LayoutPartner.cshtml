<!DOCTYPE html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="~/Images/logo2.png">
    <link href="~/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="~/vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">
    <link href="~/css/sb-admin.css" rel="stylesheet">
    <title>@ViewBag.Title</title>
</head>
<body id="page-top">
    <nav class="navbar navbar-expand navbar-dark bg-dark static-top">
        <span id="dark" class="nav-link">@Model.Partner.CompanyName</span>
        <ul class="navbar-nav ml-auto ml-md-0">
            <a class="dropdown-item" href="/main/partner/settings/@Model.Partner.Id" >პარამეტრები</a>
            <a class="dropdown-item" href="/main/login/logout">გამოსვლა</a>
        </ul>
    </nav>
    <div id="wrapper">
        <ul class="sidebar navbar-nav">
            <li class="nav-item">
                <a class="nav-link" href="/main/partner/dashboard/@Model.Partner.Id">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>პროდუქტები</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/main/partner/CallList/@Model.Partner.Id">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>ზარის განხორციელება</span>
                </a>
            </li>
        </ul>
        <div id="content-wrapper">
            @RenderBody()
        </div>
    </div>
    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/vendor/chart.js/Chart.min.js"></script>
    <script src="~/vendor/datatables/jquery.dataTables.js"></script>
    <script src="~/vendor/datatables/dataTables.bootstrap4.js"></script>
    <script src="~/js/sb-admin.js"></script>
    <script src="~/js/demo/datatables-demo.js"></script>
    <script src="~/js/AddProduct.js"></script>
    <script src="~/js/Upload Images.js"></script>
    <script src="~/js/EditProduct.js"></script>
</body>
</html>